<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.benumali.android.headachetracker" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".HeadacheListActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HeadachePagerActivity"
                  android:label="@string/app_name"
                android:parentActivityName=".HeadacheListActivity">

        </activity>
        <activity android:name=".HeadacheGraphActivity"
                android:parentActivityName=".HeadacheListActivity">
        </activity>
    </application>

</manifest>
